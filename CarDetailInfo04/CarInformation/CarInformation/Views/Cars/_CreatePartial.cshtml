@model CarInformation.Models.Car_Parts
@using (Html.BeginForm("AddQualifiactions", "Car", FormMethod.Post, new { id = "frm2" }))
{
    <div class="form-group row">
        <div class="col-3">
            <label>Engine Type</label>
            @Html.TextBoxFor(x => x.Engine_type, new { @class = "form-control" })
            @Html.ValidationMessageFor(x => x.Engine_type, "", new { @class = "text-danger" })
        </div>
        <div class="col-3">
            <label>Number Of Door</label>
            @Html.TextBoxFor(x => x.Number_of_doors, new { @class = "form-control" })
            @Html.ValidationMessageFor(x => x.Number_of_doors, "", new { @class = "text-danger" })
        </div>
        <div class="col-2">
            <label>Transmission</label>
            @Html.TextBoxFor(x => x.Transmission, new { @class = "form-control" })
            @Html.ValidationMessageFor(x => x.Transmission, "", new { @class = "text-danger" })
        </div>
        <div class="col-3">
            <label>Exhaust_System</label>
            @Html.TextBoxFor(x => x.Exhaust_System, new { @class = "form-control" })
            @Html.ValidationMessageFor(x => x.Exhaust_System, "", new { @class = "text-danger" })
        </div>
        <div class="col-1">

            <button type="button" id="add" class="btn btn-secondary btn-sm mt-4">Add</button>
        </div>
    </div>
}
